<h1 align="center">LOG IN</h1>
<%= form_for :session do |f|%>
    <p>
        <%= f.label :email %><br/>
        <%= f.email_field :email %>
    </p>
     <p>
        <%= f.label :password %><br/>
        <%= f.password_field :password %>
    </p>
    <p>
        <%= f.submit "Log in" %>
    </p>

<% end %>
<%= link_to "Back to article Listing", articles_path %>